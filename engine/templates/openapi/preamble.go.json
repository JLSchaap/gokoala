{{- /*gotype: github.com/PDOK/gokoala/engine.TemplateData*/ -}}
{
  "openapi": "3.0.0",
  "info": {
    "title": "{{ .Config.Title }}",
    "description": "{{ .Config.Abstract }} \n\n\n\n {{ .Config.Support }}",
    "version": "1.0",
    "contact": {
      {{/* empty contact information to overwrite default OGC info */}}
      "name": "",
      "email": "",
      "url": ""
    },
    "termsOfService": "",
